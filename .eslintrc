{
  
  "root": true,
  "plugins": [
    "prettier"
  ],
  "extends": [
    "eslint:recommended",
    "prettier"
  ],
  "parser": "@typescript-eslint/parser",
  "rules": {
    "strict": "error",
    "no-unused-vars": 0,
    "no-undef":0
    
    
     
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
},
  "env": {
    "node": true,
    "es6": true
  }
    
}